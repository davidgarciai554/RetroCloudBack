// .vscode/launch.json
{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "FastAPI: Uvicorn",
        "type": "debugpy",           
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "main:app",
          "--reload",
          "--env-file",
          ".env"
        ],
        "console": "integratedTerminal",
        "jinja": true
      }
    ]
  }
  